#include <iostream>
#include<bits/stdc++.h>
#include<math.h>
using namespace std;




int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int num;
	    cin>>num;
	    int count=1;
	    if(num==1)
	    {
	        cout<<num<<"\n";
	    }
	    else
	    {
	    for(int i=3;i<=num;i*=3)
	    {
	       if(i<=num)
	       {
	           count++;
	       }
	    }
	    cout<<count<<"\n";
	    }
	}
	return 0;
}
