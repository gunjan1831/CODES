 
    int toyCount(int N, int K, int arr[])
    {
        // code here
        sort(arr,arr+N);
        int count=0;
        //int sum=0;
        for(int i=0;i<N;i++)
        {
            K=K-arr[i];
            if(K>=0)
            {
                count++;
            }
        }
        return count;
    }
