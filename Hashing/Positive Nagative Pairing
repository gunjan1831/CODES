vector <int> findPairs(int arr[], int n) 
    {
        // code here
        vector<int> ans;
        unordered_map<int,int>mp;
        for(int i=0;i<n;i++)
        {
            
                mp[abs(arr[i])]++;
                if(mp[abs(arr[i])]==2)
                {
                    ans.push_back(-(abs(arr[i])));
                    ans.push_back(abs(arr[i]));
                }
            
        }
        
        
        return ans;
    }
