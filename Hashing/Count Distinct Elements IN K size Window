vector <int> countDistinct (int A[], int n, int k)
{
vector<int> ans;
unordered_map<int,int> mp;

for(int i=0;i<k;i++)
{
    mp[A[i]]++;    //first window
}
ans.push_back(mp.size());

for(int i=1;i<n-k+1;i++)
{
    mp[A[i-1]]--;  //deleteing first element
    if(mp[A[i-1]]<=0)
    {
        mp.erase(A[i-1]);
    }
    
    mp[A[i+k-1]]++;
    ans.push_back(mp.size());
}
return ans;
}
