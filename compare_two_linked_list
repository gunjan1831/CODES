bool compare_lists(SinglyLinkedListNode* head1, SinglyLinkedListNode* head2) {

SinglyLinkedListNode *p=head1;
//SinglyLinkedListNode *r=head1;
SinglyLinkedListNode *q=head2;
    
    
    
    
    while(p!=NULL&&q!=NULL)
    {
        if(p->data==q->data)
        {
            
            p=p->next;
            q=q->next;
        }
        else
        {
            return 0;
        }
    }
    if(p==NULL&&q==NULL)
    {
        return 1;
}
    return 0;

    
   
}
