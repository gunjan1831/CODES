#include <iostream>
using namespace std;

int main() {
	//code
	int t;
	cin>>t;
	while(t--)
	{
	    string s1,s2;
	    cin>>s1>>s2;
	    for(int i=0;i<s1.length();i++)
	    {
	        s1[i]=tolower(s1[i]);
	    }
	    for(int i=0;i<s2.length();i++)
	    {
	        s2[i]=tolower(s2[i]);
	    }
	    int count=0;
	    for(int i=0;i<s1.length();i++)
	    {
	        if(s1[i]==s2[i])
	        {
	            count++;
	        }
	    }
	    cout<<count<<"\n";
	}
	return 0;
}
