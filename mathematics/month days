
#include <iostream>
#include<bits/stdc++.h>
#include<math.h>
using namespace std;




int main()
{
   string s;
   cin>>s;
   
   int a= s[3]-'0';
   int b=s[4]-'0';
   if(a==0)
   {
       if(b==1|| b==3|| b==5||b==7||b==8)
       {
           cout<<31<<" ";
       }
       else
       if(b==2)
       {
           cout<<28;
       }
       else
       {
           cout<<30;
       }
   }
   else
   {
       if(b==0 ||b==2)
       {
           cout<<31;
       }
       else
       {
           cout<<30;
       }
   }
}
