You don't need any algorithm for that . simple code which works fine is as follows:
#include <iostream>
#include<bits/stdc++.h>
using namespace std;


int main() {
	//code
	int t,s;
	cin>>t;
	while(t--)
	{
	    cin>>s;
	    int c1=0,c2=0;
	    int a[s];
	    for(int i=0;i<s;i++)
	    {
	        cin>>a[i];
	        if(a[i]==1)
	        {
	            c1++;
	        }
	        else
	        {
	            c2++;
	        }
	        
	        //cout<<a[s];
	    }
	    for(int i=0;i<c2;i++)
	    {
	        cout<<"0 ";
	    }
	    for(int j=0;j<c1;j++)
	    {
	        cout<<"1 ";
	    }
	    cout<<"\n";
	    
	}
	return 0;
}
