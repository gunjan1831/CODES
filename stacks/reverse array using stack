void reverseArray(int n, int* arr){
    
  //Your code here
  
        // Write your Code here
        
        stack<int> ans;
        for(int i=0;i<n;i++)
        {
            ans.push(arr[i]);
        }
        for(int i=0;i<n;i++)
        {
           arr[i]=ans.top();
           ans.pop();
        }
        
    
  
}
