void call(Node *root,vector<int> &ans,int l,int &mxlevel)
{
    if(root==NULL)
    {
        return;
    }
    
    if(mxlevel<l)
    {
        ans.push_back(root->data);
        mxlevel=l;
    }
    call(root->left,ans,l+1,mxlevel);
    call(root->right,ans,l+1,mxlevel);
}
vector<int> leftView(Node *root)
{
  vector<int> ans;
  int level=1;
  int mxlevel=0;
  call(root,ans,level,mxlevel);
  return ans;
}
